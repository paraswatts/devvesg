{
  "name": "devvesg-test",
  "version": "1.0.0",
  "description": "DevvESG Test Environment",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-tools/graphql-file-loader": "^7.3.4",
    "@graphql-tools/load": "^7.5.2",
    "@hubspot/api-client": "^6.0.1-beta2",
    "@mikro-orm/cli": "^4.5.9",
    "@mikro-orm/core": "^4.5.9",
    "@mikro-orm/postgresql": "^4.5.9",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@types/jest": "^27.4.1",
    "@types/multer": "^1.4.7",
    "@types/sns-validator": "^0.3.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.1",
    "apollo": "^2.33.9",
    "apollo-resolvers": "^1.4.2",
    "apollo-server-express": "^3.6.3",
    "aws-sdk": "^2.1005.0",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dd-trace": "^2.7.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "handlebars": "^4.7.7",
    "i18next": "^21.10.0",
    "i18next-http-middleware": "^3.2.1",
    "i18next-node-fs-backend": "^2.1.3",
    "jest": "^27.5.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "redis": "^4.3.1",
    "sns-validator": "^0.3.4",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "uuid": "^8.3.2",
    "winston": "^3.4.0",
    "winston-json-formatter": "^0.10.0",
    "zod": "^3.9.5"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "cpy './server/gql/typeDefs/*.graphql' dist/gql/typeDefs && cpy './server/emails/templates/**/*.hbs' dist/emails/templates",
    "start": "node dist/index.js",
    "start:local": "ts-node-dev server/index.ts",
    "test": "NODE_ENV=test yarn db:reset:test && NODE_ENV=test S3_BUCKET_NAME=placeholder jest --verbose --silent --maxWorkers=1 --detectOpenHandles --forceExit --coverage",
    "db:seed": "ts-node-dev ./server/seed.ts",
    "db:seed-quiz": "ts-node-dev ./server/seed-quiz.ts",
    "db:reset": "mikro-orm schema:drop -r --fk-checks --drop-migrations-table && mikro-orm migration:up",
    "db:reset:local": "mikro-orm schema:drop -r --drop-migrations-table && mikro-orm migration:up",
    "db:reset:test": "NODE_ENV=test mikro-orm schema:drop -r --drop-migrations-table && NODE_ENV=test mikro-orm migration:up && NODE_ENV=test ts-node-dev ./server/tests/test-seed.ts",
    "db:migrate": "mikro-orm migration:up",
    "db:migrate:create": "mikro-orm migration:create",
    "gql:codegen": "graphql-codegen --config ./codegen.yml && apollo client:codegen --endpoint=http://localhost:5000/graphql __gql__ --target=typescript --globalTypesFile=./client/src/gql/__generated__/globalTypes.ts --passthroughCustomScalars",
    "write-secrets": "APP_SECRETS=${APP_SECRETS} ts-node-dev ./server/write-secrets.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeatdevvio/devvesg-test.git"
  },
  "keywords": [
    "mern",
    "aws",
    "ec2",
    "postgresql",
    "react",
    "express",
    "devvesg"
  ],
  "author": "Mike DiPetrillo",
  "bugs": {
    "url": "https://github.com/mikeatdevvio/devvesg-test/issues"
  },
  "homepage": "https://github.com/mikeatdevvio/devvesg-test#readme",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.10.2",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "cliui": "^6.0.0",
    "cpy-cli": "^4.1.0",
    "typescript": "4.4.3"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./server/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}

    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@headlessui/react": "^1.6.0",
    "@react-google-maps/api": "^2.12.2",
    "@tailwindcss/forms": "^0.4.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.25",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-datepicker": "^4.1.7",
    "@types/react-dom": "^17.0.9",
    "@types/react-pdf": "^5.7.2",
    "ag-grid-community": "^27.1.0",
    "ag-grid-react": "^27.1.0",
    "antd": "^4.19.2",
    "apollo-link-scalars": "^4.0.1",
    "autoprefixer": "^10.4.2",
    "axios": "^0.25.0",
    "clsx": "^1.1.1",
    "graphql": "^16.3.0",
    "graphql-scalars": "^1.17.0",
    "i18next": "^21.6.16",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.0",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-cookie-consent": "^7.3.1",
    "react-country-region-selector": "^3.4.0",
    "react-datepicker": "^4.3.0",
    "react-dom": "^17.0.2",
    "react-ga4": "^1.4.1",
    "react-hook-form": "^7.24.1",
    "react-i18next": "^11.16.8",
    "react-pdf": "^5.7.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-select": "^5.2.1",
    "react-select-event": "^5.3.0",
    "react-textfit": "^1.1.1",
    "react-toastify": "^8.0.3",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.4.3",
    "use-debounce": "^7.0.1",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:import/recommended",
      "plugin:import/typescript"
    ],
    "rules": {
      "testing-library/render-result-naming-convention": "off",
      "import/no-unresolved": "off",
      "import/no-relative-parent-imports": "error",
      "sort-imports": [
        "error",
        {
          "ignoreDeclarationSort": true
        }
      ],
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            "internal"
          ],
          "pathGroups": [
            {
              "pattern": "react",
              "group": "external",
              "position": "before"
            },
            {
              "pattern": "@testing-library/**",
              "group": "external",
              "position": "before"
            },
            {
              "pattern": "src/**/*.{css,scss}",
              "group": "internal",
              "position": "after"
            },
            {
              "pattern": "src/**/*.{png,svg,jpg}",
              "group": "internal",
              "position": "after"
            },
            {
              "pattern": "src/**",
              "group": "internal"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "react",
            "@testing-library/**"
          ],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "@fortawesome/fontawesome-common-types": "^6.1.1",
    "eslint-plugin-import": "^2.25.4",
    "msw": "^0.39.1",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "sass": "^1.45.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/routes/admin",
      "__gql__",
      "__generated__"
    ]
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  }
}
